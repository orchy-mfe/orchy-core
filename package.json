{
  "name": "orchy-core",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "pnpm build",
    "w:models": "pnpm --filter models",
    "w:be": "pnpm --filter be",
    "w:wc": "pnpm --filter wc",
    "w:pb": "pnpm --filter page-builder",
    "lint": "npm-run-all -p 'w:models lint' 'w:be lint' 'w:wc lint' 'w:pb lint'",
    "lint:fix": "npm-run-all -p 'w:models lint:fix' 'w:be lint:fix' 'w:wc lint:fix' 'w:pb lint:fix'",
    "dev": "npm-run-all -p 'w:wc dev' 'w:be dev'",
    "test": "npm-run-all -p 'w:be test' 'w:wc test' 'w:pb test'",
    "build": "npm-run-all 'w:models build' 'w:pb build' 'w:be build' 'w:wc build'"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
