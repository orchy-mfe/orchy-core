{
  "name": "@orchy-mfe/page-builder",
  "version": "0.1.0",
  "description": "Build page nodes based on specific configuration",
  "module": "dist/page-builder.mjs",
  "main": "dist/page-builder.umd.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=v18.12.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build && tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "dev": "pnpm build --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "rxjs": "^7.5.7"
  },
  "devDependencies": {
    "@orchy-mfe/models": "workspace:^",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.0"
  },
  "author": "Matteo Pietro Dazzi",
  "license": "MIT"
}
